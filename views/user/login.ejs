<h1 id = logintitle>Login</h1>
<div id = "login">
<form action="/user/login" method="post">


  <input type="text"
         name="user[username]"
         placeholder="usernname"
         id = "username"></br>

  <input type="text"
         name="user[password]"
         placeholder="Password"
